{
  "pretrained_model_name_or_path": "path/to/sdxl-base-1.0",
  "train_data_dir": "datasets/your_dataset",
  "output_dir": "outputs/models",
  "output_name": "sdxl_lora",
  "save_model_as": "safetensors",
  "save_precision": "fp16",
  "seed": 42,
  "resolution": "1024,1024",
  "enable_bucket": true,
  "min_bucket_reso": 256,
  "max_bucket_reso": 2048,
  "bucket_resolutions": [
    [512, 2048],
    [512, 1984],
    [512, 1920],
    [512, 1856],
    [768, 1792],
    [768, 1728],
    [896, 1664],
    [896, 1600],
    [1024, 1536],
    [1024, 1472],
    [1088, 1408],
    [1088, 1344],
    [1152, 1280],
    [1216, 1216],
    [1280, 1152],
    [1344, 1088],
    [1408, 1088],
    [1472, 1024],
    [1536, 1024],
    [1600, 896],
    [1664, 896],
    [1728, 768],
    [1792, 768],
    [1856, 512],
    [1920, 512],
    [1984, 512],
    [2048, 512]
